let age = 25;

if(age>=18)
{
    console.log("You can go to goa");
}
else
{
    console.log("You cannot go to goa");
}   

let marks = 90;
if(marks>=90)
{
    console.log("You got A grade");
}else if(marks>=80)
{
    console.log("You got B grade");
}else if(marks>=70)
{
    console.log("You got C grade");
}else if(marks>=60)
{
    console.log("You got D grade");
}else   {
    console.log("You got F grade");
}